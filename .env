PROJECT_NAME=mahidhargopalabhatla-url-shortner-api

PHP_IMAGE=php74
DOCUMENT_ROOT=./www
APACHE_DOCUMENT_ROOT=/var/www/html
VHOSTS_DIR=./config/vhosts
APACHE_LOG_DIR=./www/logs/apache2
PHP_LOG_DIR = /var/log
PHP_LOG_FILE = /var/log/php/php.log
PHP_INI=./config/php/php.ini
DEFAULT_TIMEZONE = America/New_York


DATABASE_IMAGE=mysql
MYSQL_DATA_DIR=./data/mysql
MYSQL_LOG_DIR=./www/logs/mysql

# If you already have the port 80 in use, you can change it (for example if you have Apache)
HOST_MACHINE_UNSECURE_HOST_PORT=80

# If you already have the port 443 in use, you can change it (for example if you have Apache)
HOST_MACHINE_SECURE_HOST_PORT=443

HOST_MACHINE_MEMCACHE_PORT=11211

# If you already have the port 3306 in use, you can change it (for example if you have MySQL)
HOST_MACHINE_MYSQL_PORT=3306

# If you already have the port 8080 in use, you can change it (for example if you have PMA)
HOST_MACHINE_PMA_PORT=8080



# MySQL root user password
MYSQL_ROOT_PASSWORD=urlshortner

# Database settings: Username, password and database name
#
# If you need to give the docker user access to more databases than the "docker" db 
# you can grant the privileges with phpmyadmin to the user.
MYSQL_USER=docker
MYSQL_PASSWORD=docker
MYSQL_DATABASE=url_shortner
